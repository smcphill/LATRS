<h1>Editing source</h1>

<% form_for([:admin, @source]) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :name %><br />
    <%= f.text_field :name %>
  </p>
  <p>
    <%= f.submit "Update" %>
  </p>
<% end %>

<%= link_to 'Show', [:admin, @source] %> |
<%= link_to 'Back', admin_sources_path %>
